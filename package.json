  "engines": {
    "node": ">= 4"
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/chai-as-promised": "^7.1.0",
    "@types/chai-string": "^1.4.0",
    "@types/mocha": "^2.2.48",
    "@types/node": "^9.4.6",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-string": "^1.4.0",
    "chai-withintoleranceof": "^1.0.1",
    "mocha": "^5.0.1",
    "prettier": "^1.11.0",
    "rimraf": "^2.6.2",
    "seedrandom": "^2.4.3",
    "serve": "^6.5.1",
    "source-map-support": "^0.5.3",
    "timekeeper": "^2.0.0",
    "typedoc": "^0.10.0",
    "typescript": "^2.7.2"
  }
